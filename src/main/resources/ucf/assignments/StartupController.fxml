<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.StartupController">
   <children>
      <Label alignment="CENTER" prefHeight="125.0" prefWidth="600.0" text="Welcome! Open an existing list, or create a new blank list." textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="createNewButton" layoutX="62.0" layoutY="126.0" mnemonicParsing="false" onAction="#createNewList" prefHeight="26.0" prefWidth="205.0" text="Create new" AnchorPane.leftAnchor="20.0" />
      <Button fx:id="openExistingButton" layoutX="348.0" layoutY="126.0" mnemonicParsing="false" onAction="#openExistingList" prefHeight="26.0" prefWidth="205.0" text="Open existing" AnchorPane.rightAnchor="20.0" />
   </children>
</AnchorPane>
